PPC ppc-adir7 (PPCAdirSeven)
{
 P0:r1=1; P0:r2=1;
 P0:r5=x; P1:r5=x;
 P0:r6=y; P1:r6=y;
 x = 0; y = 0;
}

    P0       |      P1       ;
             |               ;
stw r1,0,r5  | lwz r3,0,r6 ;
             | mr  r10, r3 ;
sync         | xor r3,r3,r3 ;
stw r2,0,r6  | lwzx r4,r3,r5 ;


exists (1:r4=0 /\ 1:r10=1)
<<
genprog generated/ppc-adir7-prog.tex
show 1
essdump generated/ppc-adir7-ess.dot
>>
<<
show 0
show interesting true
readfrom generated/ppc-adir7-rf.dot
