PPC ppc-adir8 (PPCAdirEight)
{
 P0:r10=1; P1:r10=1; P2:r11=2;
 P0:r5=x; P1:r5=x; P2:r5=x;
 P0:r6=y; P1:r6=y; 
 x = 0; y = 0;
}

    P0       |      P1    |      P2        ;
stw r10,0,r5 | stw r10,0,r6 | stw r11,0,r5 ;
sync         | sync         |              ;
