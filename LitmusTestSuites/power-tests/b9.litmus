PPC b9 (BasicNine)
"Simple sync test, avoid reg duplicates"
{0:r6=1; 0:r4=x; 0:r5=y; 1:r4=x; 1:r5=y; x=0; y=0;
 1:r7=z; 1:r8=t;}

P0               | P1                ;
 stw r6,0(r4)    | lwz r2,0(r5)      ;
 lwsync          | stw r2,0(r8)     ;
 stw r6,0(r5)    | lwz r1,0(r4)      ;
                 | stw r1,0(r7)     ;

exists (t=1 /\ z=0)
