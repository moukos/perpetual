X86 MP
"Fre PodWR Fre PodWR"
{ x=0; y=0; }
 P0          | P1          ;
             | MOV EDX,$0  ;
 MOV [x],$1  | MOV ECX, [y];
 MOV [y],$1  | MOV EAX, [x];
             | INC EAX     ;
	     | CMP EAX,ECX ;
	     | JNE END     ;
	     | MOV EDX, $1 ;
	     | END:        ;
 locations [x;y;]
 exists (1:ECX=1 /\ 1:EAX=1)