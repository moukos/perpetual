ARM LB+dsb.st+ctrl
"DSB.STdRW Rfe DpCtrldW Rfe"
Cycle=Rfe DSB.STdRW Rfe DpCtrldW
Prefetch=0:x=F,0:y=W,1:y=F,1:x=W
Com=Rf Rf
Orig=DSB.STdRW Rfe DpCtrldW Rfe
{
%x0=x; %y0=y;
%y1=y; %x1=x;
}
 P0           | P1           ;
 LDR R0,[%x0] | LDR R0,[%y1] ;
 DSB ST       | CMP R0,R0    ;
 MOV R1,#1    | BNE LC00     ;
 STR R1,[%y0] | LC00:        ;
              | MOV R1,#1    ;
              | STR R1,[%x1] ;
