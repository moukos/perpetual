ARM S+isb+dmb
"ISBdWW Rfe DMBdRW Wse"
Cycle=Rfe DMBdRW Wse ISBdWW
{
%x0=x; %y0=y;
%y1=y; %x1=x;
}
 P0            | P1            ;
 MOV R0, #2    | LDR R0, [%y1] ;
 STR R0, [%x0] | DMB           ;
 ISB           | MOV R1, #1    ;
 MOV R1, #1    | STR R1, [%x1] ;
 STR R1, [%y0] |               ;
exists
(x=2 /\ 1:R0=1)
